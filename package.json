    "ts-jest": "^27.0.5",
    "ts-node": "^10.9.1",
    "typescript": "^4.4.3"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "testMatch": [
      "**/__tests__/**/*.+(ts|tsx)",
